<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back"
    android:minWidth="25px"
    android:minHeight="25px">
    <Space
        android:layout_width="match_parent"
        android:layout_height="99.0dp"
        android:id="@+id/space1" />
    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="219.0dp"
        android:id="@+id/linearLayout1">
        <Space
            android:layout_width="30dp"
            android:layout_height="match_parent"
            android:id="@+id/space2" />
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="169.0dp"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout2">
            <ImageButton
                android:src="@drawable/shoppingcart"
                android:layout_width="108.0dp"
                android:layout_height="84.5dp"
                android:scaleType="centerInside"
                android:background="@color/mtrl_btn_transparent_bg_color"
                android:id="@+id/imageView1"
                android:layout_marginLeft="18.5dp"
                android:layout_marginTop="18.5dp" />
            <TextView
                android:text="Large Text"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView1" />
            
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="173.5dp"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout3">
            <Space
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:id="@+id/space3"
                android:layout_marginBottom="18.5dp" />
            <ImageButton
                android:src="@drawable/clients"
                android:layout_width="141.5dp"
                android:layout_height="103.0dp"
                android:scaleType="centerInside"
                android:background="@color/mtrl_btn_transparent_bg_color"
                android:id="@+id/client"
                android:layout_marginLeft="13.5dp"
                android:layout_marginTop="27.5dp" />
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
