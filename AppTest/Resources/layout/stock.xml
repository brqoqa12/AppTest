<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:background="@android:color/white"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="71.5dp"
        android:id="@+id/linearLayout1" >
        <ImageButton
            android:background="@drawable/backbtn"
            android:layout_width="68.0dp"
            android:layout_height="match_parent"
            android:id="@+id/back" />
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="634.7dp"
        android:id="@+id/linearLayout2">
        <Space
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:id="@+id/space1" />
        <TextView
            android:text="     Stock :"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="321.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView1"
            android:layout_marginLeft="0.0dp"
            android:layout_marginRight="0.0dp" />
        <Spinner
            android:layout_width="match_parent"
            android:layout_height="40.0dp"
            android:id="@+id/spinner1"
            android:layout_marginBottom="0.0dp" />
        <Space
            android:layout_width="match_parent"
            android:layout_height="25.9dp"
            android:id="@+id/space2"
            android:layout_marginBottom="0.0dp" />
        <com.google.android.material.textfield.TextInputEditText
            android:text="Réference"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/textInputEditText1"
            android:layout_marginLeft="102.1dp" />
        <Button
            android:text="Chercher"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/button1"
            android:layout_marginLeft="121.1dp"
            android:layout_marginRight="128.4dp" />
        <Space
            android:layout_width="match_parent"
            android:layout_height="44.2dp"
            android:id="@+id/space3" />
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="43.4dp"
            android:id="@+id/linearLayout3" >
            <TextView
                android:text="Quantité"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:id="@+id/textView3"
                android:layout_marginRight="40.8dp" />
            <TextView
                android:text="danone"
                android:layout_width="196.6dp"
                android:layout_height="match_parent"
                android:id="@+id/textView4" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="51.8dp"
            android:id="@+id/linearLayout4" >
            <TextView
                android:text="Prix"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:id="@+id/textView2"
                android:layout_marginRight="47.6dp" />
            <TextView
                android:text="50"
                android:layout_width="241.5dp"
                android:layout_height="match_parent"
                android:id="@+id/textView5" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
